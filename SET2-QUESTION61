

USE POPULATION;
CREATE TABLE point
(
  x INT,
  CONSTRAINT pk_point PRIMARY KEY (x)
);

INSERT INTO point VALUES(-1);
INSERT INTO point VALUES(0);
INSERT INTO point VALUES(2);


USE POPULATION;
CREATE TABLE point
(
  x INT,
  CONSTRAINT pk_point PRIMARY KEY (x)
);

INSERT INTO point VALUES(-1);
INSERT INTO point VALUES(0);
INSERT INTO point VALUES(2);


SELECT X FROM (SELECT X, LEAD(X) OVER(ORDER BY X)-X AS DIFF FROM POINT) TEMP WHERE DIFF=1;
